//main------------------------------------------------------------------------------------------------------------------------------------------------------------------------
public class CardTest {

	public static void main(String[] args) {
		// TODO 자동 생성된 메소드 스텁
		Card spade1 = new Card("spade",1);
		System.out.println(spade1.kind);
		System.out.println(spade1.number);
		
		Card clover1 = new Card("clover",1);
		System.out.println(clover1.kind);
		System.out.println(clover1.number);
		
		Card heart1 = new Card("heart1",1);
		System.out.println(heart1.kind);
		System.out.println(heart1.number);
		
		System.out.println(heart1.height);
		System.out.println(Card.height);
		

	}

}

//클래스------------------------------------------------------------------------------------------------------------------------------------------------------------------------
public class Card {
	String kind;//카드의 종류
	int number;//카드의 숫자
	static int height = 200;//카드의 높이
	static int width = 200;//카드의 넓이
	
	Card(String kind, int number){
		this.kind = kind;
		this.number = number;
	}

}

