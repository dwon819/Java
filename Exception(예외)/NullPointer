package com.exception;

public class NullPointer {

	public static void main(String[] args) {
		// TODO 자동 생성된 메소드 스텁
		String data = null;
		try {
			System.out.println(data.toString());
		}catch (NullPointerException e) {
			// TODO: handle exception
			data = "시";
			System.out.println(data.toString());
		}
		

	}

}
