package interface1;

public class AnonymousExample {

	public static void main(String[] args) {
		// TODO 자동 생성된 메소드 스텁
		Anonymous anony = new Anonymous();
		anony.field.run();
		anony.method1();
		anony.method2(new Vehicle() {
			
			@Override
			public void run() {
				// TODO 자동 생성된 메소드 스텁
				System.out.println("트럭이 달립니다");
			}
		});

	}

}
