package Example;

public class LeastJob implements Scheduler {
	public LeastJob() {//최소작업
		// TODO 자동 생성된 생성자 스텁
	}

	@Override
	public void getNextCall(CustomCall c) {
		// TODO 자동 생성된 메소드 스텁
		System.out.println(c.name+"님 비어있는 상담원에게 연결해 드리겠습니다.");
		
		
	}

	@Override
	public void sendCallToAgent(CustomCall c) {
		// TODO 자동 생성된 메소드 스텁
		System.out.print(c.name+"님 ");
		System.out.println("상담원이 연결되었습니다.");
		
	}

	

}
