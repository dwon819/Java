package Example;

public class RoundRobin implements Scheduler{

	public RoundRobin() {//순서대로
		// TODO 자동 생성된 생성자 스텁
	}

	@Override
	public void getNextCall(CustomCall c) {
		// TODO 자동 생성된 메소드 스텁
		System.out.println(c.name+"님 상담원이 모두 통화중입니다.순서대로 연결 됩니다");
	
		
		
	}

	@Override
	public void sendCallToAgent(CustomCall c) {
		// TODO 자동 생성된 메소드 스텁
		System.out.print(c.name+"님 ");
		System.out.println("상담원에게 연결 되었습니다.");
		
		
	}

	

}
